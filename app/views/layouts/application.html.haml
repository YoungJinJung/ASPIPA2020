%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{name: 'viewport', content: 'width=device-width, initial-scale=1.0'}/
    %title APSIPA ASC 2020 - Asia-Pacific Signal and Information Processing Association Annual Summit and Conference 2020
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = stylesheet_link_tag    'main', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
    = stylesheet_link_tag    "https://fonts.googleapis.com/css?family=Lato"
  %body
    %nav{class: 'navbar navbar-default navbar-fixed-top'}
      %div{class: 'container-fluid'}
        %div{class: 'navbar-header'}
          %button{type: 'button', class: 'navbar-toggle collapsed', 'data-toggle' => 'collapse', 'data-target' => '#sitenav'}
            %span{class: 'icon-bar'}
            %span{class: 'icon-bar'}
            %span{class: 'icon-bar'}
          %a{class: 'navbar-brand', href: '/'}
            %font{color: 'white'}
              %b APSIPA ASC 2020
        %div{class: 'collapse navbar-collapse pull-right', id: 'sitenav'}
          %ul{class: 'nav navbar-nav'}
            = render partial: 'general', locals: {title: 'General'}
            = render partial: 'calls', locals: {title: 'Calls'}
            = render partial: 'attendees', locals: {title: 'Attendees'}
            = render partial: 'program', locals: {title: 'Program'}
            -#= render partial: 'conference', locals: {title: 'Conference'}
          -#= render partial: 'industry', locals: {title: 'Industry'}
          -#= render partial: 'authors', locals: {title: 'Authors'}
          -#= render partial: 'students', locals: {title: 'Students'}

    %div{class: 'container-fluid', id: 'main-content'}
      = yield
    %div{class: 'container-fluid footer'}
      APSIPA ASC 2020
